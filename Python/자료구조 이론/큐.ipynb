    "\n",
    "queue_list = list()\n",
    "\n",
    "def enqueue(data): # 입력\n",
    "    queue_list.append(data)\n",
    "\n",
    "def dequeue(): # 출력\n",
    "    data = queue_list[0]\n",
    "    del queue_list[0]\n",
    "    return data"
