   "source": [
    "food_times = list(map(int, input().split()))\n",
    "k = int(input())\n",
    "\n",
    "while k > 0:\n",
    "    for i in food_times:\n",
    "        if i != 0:\n",
    "            i -= 1\n",
    "            k -= 1\n",
    "        else:\n",
    "            continue\n",
    "\n",
    "print(food_times)\n"
